{
  "name": "eda-te-reo",
  "version": "0.1.0",
  "description": "Simple Te Reo translation API for students",
  "main": "build/index.js",
  "scripts": {
    "build": "babel src --out-dir build",
    "dev": "nodemon --exec babel-node --inspect src/index",
    "postinstall": "npm run build",
    "start": "node build/index"
  },
  "keywords": [
    "maori",
    "reo",
    "enspiral",
    "academy",
    "eda"
  ],
  "author": "EDA",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "google-translate-api": "^2.2.2",
    "kcors": "^2.2.0",
    "knex": "^0.12.6",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-router": "^7.0.1",
    "pg": "^6.1.2"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  },
  "engines": {
    "node": ">7.0"
  },
  "babel": {
    "passPerPreset": true,
    "presets": [
      {
        "plugins": [
          "transform-runtime"
        ]
      },
      {
        "passPerPreset": false,
        "presets": [
          "latest"
        ]
      }
    ]
  }
}
